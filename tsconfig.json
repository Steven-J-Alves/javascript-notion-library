{
  "ts-node": { "compilerOptions": { "module": "es6", "types": ["node"] } },
  "compilerOptions": {
    "outDir": "./dist-ts",
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["esnext", "DOM", "DOM.Iterable", "WebWorker"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitThis": false,
    "types": [
      "mocha",
      "node"
   ],
   "typeRoots": [
     "./node_modules/@types"
   ]
  },
  "include": [
    "./src/*.ts",
    "./src/**/*.ts",
    ""
, "src/index.ts", "src/page-notion-api.ts", "src/search-notion-api.ts", "src/user-notion-api.ts"  ],
  "exclude": ["node_modules/", "dist/", "lib/"]
}
